<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <% include ../../partials/head %>
  </head>
  <body>
    <%# Screen 25 %>
    <h1>Manage Event</h1>
    <form class="" action="/manager/events" method="get">
      <div class="">
        <div class="">
          <label for="Name">Name</label>
          <input type="text" name="name" value=""/>
        </div>
        <div class="">
          <label for="Name">Description Keyword</label>
          <input type="text" name="keyword" value=""/>
        </div>
      </div>
      <div class="">
        <div class="">
          <label for="Name">Start Date</label>
          <input type="text" name="startDate" value=""/>
        </div>
        <div class="">
          <label for="Name">End Date</label>
          <input type="text" name="endDate" value=""/>
        </div>
      </div>
      <div class="">
        <div class="">
          <label for="duration">Duration Range</label>
          <input type="text" name="lowerDuration" value=""/>
          &nbsp;&mdash;&nbsp;
          <input type="text" name="upperDuration" value=""/>
        </div>
        <div class="">
          <label for="visitRange">Total</label>
          <input type="text" name="lowerVisit" value=""/>
          &nbsp;&mdash;&nbsp;
          <input type="text" name="upperVisit" value=""/>
        </div>
      </div>
      <div class="">
        <div class="">
          <label for="revenueRange">Total Revenue Range</label>
          <input type="text" name="lowerRevenue" value=""/>
          &nbsp;&mdash;&nbsp;
          <input type="text" name="upperRevenue" value=""/>
        </div>
      </div>
    </form>
    <div class="">
      <button type="button" name="filter">Filter</button>
      <button type="button" name="create">Create</button>
      <button type="button" name="edit">View/Edit</button>
      <button type="button" name="delete">Delete</button>
    </div>
    <table>
      <tr>
        <th>Name</th>
        <th>Staff Count</th>
        <th>Duration (days)</th>
        <th>Total Visits</th>
        <th>Total Revenue ($)</th>
      </tr>
      <tr>
        <td>Bus Tour</td>
        <td>4</td>
        <td>1</td>
        <td>80</td>
        <td>2000</td>
      </tr>
    </table>
    <div class="">
      <button class="btn" type="button" name="back">Back</button>
    </div>
  </body>
</html>
